{
  "name": "@most/dom-event",
  "version": "1.4.0",
  "description": "Streamlined DOM Events for most.js",
  "main": "dist/mostDomEvent.js",
  "jsnext:main": "dist/mostDomEvent.es.js",
  "module": "dist/mostDomEvent.es.js",
  "typings": "type-definitions/dom-event.d.ts",
  "files": [
    "src/*",
    "dist/*",
    "type-definitions/dom-event.d.ts"
  ],
  "scripts": {
    "build": "npm run build:dist && npm run build:min && npm run build:flow",
    "build:dist": "mkdir -p dist && rollup -c && rollup -c rollup.es.js",
    "build:min": "uglifyjs dist/mostDomEvent.js -o dist/mostDomEvent.min.js",
    "build:flow": "cpy src/index.js.flow dist --rename mostDomEvent.js.flow",
    "prepublish": "npm run build",
    "preversion": "npm run build",
    "test": "npm run test:lint && npm run test:unit && npm run test:flow",
    "test:unit": "istanbul cover _mocha",
    "test:lint": "jsinspect src && jsinspect test && eslint src test --ignore-pattern flow",
    "test:flow": "flow check",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mostjs/dom-event.git"
  },
  "keywords": [
    "most",
    "reactive",
    "event",
    "functional",
    "async",
    "dom"
  ],
  "author": "brian@hovercraftstudios.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mostjs/dom-event/issues"
  },
  "homepage": "https://github.com/mostjs/dom-event#readme",
  "devDependencies": {
    "@most/eslint-config-most": "^1.0.3",
    "assert": "^1.4.1",
    "babel-eslint": "^6.1.2",
    "buba": "^3.0.0",
    "cpy-cli": "^1.0.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "flow-bin": "^0.54.0",
    "greenkeeper-postpublish": "^1.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsinspect": "^0.8.0",
    "mocha": "^3.0.2",
    "most": "^1.0.1",
    "rollup": "0.40.0",
    "rollup-plugin-buble": "0.15.0",
    "uglify-js": "^2.7.1"
  },
  "peerDependencies": {
    "most": "^1.0.1"
  }
}
